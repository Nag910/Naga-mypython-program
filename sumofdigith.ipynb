      "source": [
        "def sum_digits(number):\n",
        "\n",
        "  sum_of_digits = 0\n",
        "  while number > 0:\n",
        "    digit = number % 10\n",
        "    sum_of_digits += digit\n",
        "    number //= 10\n",
        "  return sum_of_digits\n",
        "\n",
        "\n",
        "number = int(input(\"Enter a number: \"))\n",
        "\n",
        "\n",
        "result = sum_digits(number)\n",
        "\n",
        "\n",
        "print(\"The sum of digits is:\", result)"
      ],
